<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="com.digitalchina.itil.event.entity.EventAssign" table="EventAssign">
		<id name="id" type="long">
				<column name="id"></column>
				<generator class="native"></generator>
		</id>
		
		<many-to-one name="event" class="com.digitalchina.itil.event.entity.Event" >
            <column name="event" />
        </many-to-one>
        
        <many-to-one name="assignUser" class="com.digitalchina.info.framework.security.entity.UserInfo" >
            <column name="assignUser" />
        </many-to-one>
        
        <many-to-one name="roleId" class="com.digitalchina.info.framework.security.entity.Role" >
            <column name="roleId" />
        </many-to-one>
        <property name="assignDate" type="timestamp">
			<column name="assignDate"></column>
		</property>
		
		<property name="finishDate" type="timestamp">
			<column name="finishDate"></column>
		</property>
			
		<property name="remark" type="string">
			<column name="remark"></column>
		</property>
		
		
	</class>
</hibernate-mapping>
