<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="com.digitalchina.itil.account.entity.ITPassword" table="itil_ac_ITPassword" dynamic-insert="true" dynamic-update="true">
		<id name="id" type="long">
			<column name="id" />
			<generator class="native" />
		</id> 
		<property name="mailType" type="int">
			<column name="mailType"></column>
		</property>
		<property name="webMail" type="string">
			<column name="webMail" ></column>
		</property>
		<property name="dcMail" type="string">
			<column name="dcMail" ></column>
		</property>
	    <many-to-one name="applyId" class="com.digitalchina.itil.require.entity.AccountApplyMainTable">
		      <column name="applyId" />
		</many-to-one>
	</class>
</hibernate-mapping>
