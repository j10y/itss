<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.digitalchina.itil.config.extci.entity.BIuniverseEvent" table="itil_ci_BIuniverseEvent">
      <id name="id" type="java.lang.Long">
			  <generator class="native"/>
      </id>
			<property name="name" type="java.lang.String">
			  <column name="NAME" ></column>
			</property>
			
			<property name="biClass" type="java.lang.String">
			  <column name="BICLASS" ></column>
			</property>
			
			<property name="serviceObject" type="java.lang.String">
			  <column name="SERVICEOBJECT" ></column>
			</property>
			
			<property name="goliveDate" type="date">
			  <column name="GOLIVEDATE"></column>
			</property>
			<property name="description" type="java.lang.String">
			  <column name="DESCRIPTION" ></column>
			</property>
			
			<property name="cisn" type="java.lang.String">
			  <column name="CISN" ></column>
			</property>
			
	
  </class>
</hibernate-mapping>
