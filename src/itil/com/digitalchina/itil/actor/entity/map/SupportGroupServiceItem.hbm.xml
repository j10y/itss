<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="com.digitalchina.itil.actor.entity.SupportGroupServiceItem" table="SupportGroupServiceItem" >
        <id name="id" type="long">
            <column name="id" />
            <generator class="native" />
        </id>
  		<many-to-one name="supportGroup" class="com.digitalchina.itil.actor.entity.SupportGroup">
            <column name="supportGroup" />
        </many-to-one>
        
         <many-to-one name="serviceItem" class="com.digitalchina.itil.service.entity.ServiceItem">
            <column name="serviceItem" />
        </many-to-one>
        
	</class>
</hibernate-mapping>
