<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.zsgj.itil.config.extlist.entity.HRSAccountManger" table="itil_lst_HRSAccountManger">
      <id name="id" type="java.lang.Long">
			  <generator class="native"/>
      </id>
			<property name="itcode" type="java.lang.String">
			  <column name="ITCODE" ></column>
			</property>
			
			<property name="userName" type="java.lang.String">
			  <column name="USERNAME" ></column>
			</property>
			
			<property name="dept" type="java.lang.String">
			  <column name="DEPT" ></column>
			</property>
			
	
  </class>
</hibernate-mapping>
