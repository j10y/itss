<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.zsgj.itil.config.extci.entity.B2Binterface" table="itil_ci_B2Binterface">
      <id name="id" type="java.lang.Long">
			  <generator class="native"/>
      </id>
      
      <property name="cisn" type="java.lang.String">
			  <column name="CISN" ></column>
			</property>
			<property name="name" type="java.lang.String">
			  <column name="name" ></column>
			</property>
			<property name="description" type="java.lang.String">
			  <column name="description" ></column>
			</property>
			<many-to-one name="businessType" class="com.zsgj.itil.config.extlist.entity.B2BbusinessType">
		      <column name="businessType" />
		    </many-to-one>
		    <property name="goLiveDate" type="timestamp">
			  <column name="goLiveDate"></column>
			</property>
			<property name="partnerName" type="java.lang.String">
			  <column name="partnerName" ></column>
			</property>
			
			<property name="partnerSystem" type="java.lang.String">
			  <column name="partnerSystem"></column>
			</property>
			
			<property name="partnerAdmin" type="java.lang.String">
			  <column name="partnerAdmin" ></column>
			</property>
			<property name="partyEmail" type="java.lang.String">
			  <column name="partyEmail" ></column>
			</property>
			<property name="partyTel" type="java.lang.String">
			  <column name="partyTel" ></column>
			</property>
			<property name="DCBU" type="java.lang.String">
			  <column name="DCBU" ></column>
			</property>
			<property name="costCenter" type="java.lang.String">
			  <column name="costCenter" ></column>
			</property>
			<property name="technicalDoc" type="java.lang.String">
			  <column name="technicalDoc" ></column>
			</property>
			
	
  </class>
</hibernate-mapping>
