<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.xpsoft.oa.model.kpi.HrPaKpiitem"
        table="hr_pa_kpiitem"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="id"
            column="id"
            type="java.lang.Long"
            length="19"
        >
            <generator class="native"/>
        </id>
        <many-to-one name="pbc" not-null="false" class="com.xpsoft.oa.model.kpi.HrPaKpipbc" column="pbcId"></many-to-one>
        <many-to-one name="pi" not-null="false" class="com.xpsoft.oa.model.kpi.HrPaPerformanceindex" column="piId"></many-to-one>
        <property
            name="weight"
            type="java.lang.Double"
            update="true"
            insert="true"
            access="property"
            column="weight"
            length="20"
            not-null="false"
            unique="false"
        />
        <property
            name="result"
            type="java.lang.Double"
            update="true"
            insert="true"
            access="property"
            column="result"
            length="20"
            not-null="false"
            unique="false"
        />
        <property
            name="coefficient"
            type="java.lang.Double"
            update="true"
            insert="true"
            access="property"
            column="coefficient"
            length="20"
            not-null="false"
            unique="false"
        />
    </class>
</hibernate-mapping>
