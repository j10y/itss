<?xml version="1.0" encoding="UTF-8"?>

<process-definition  xmlns=""  name="itDevSearchRequireProcess">


	<description>
		IT开发类查询显示申请流程
	</description>


	<start-state name="提出申请">
		<description>
			start
		</description>
		<transition to="部门总监审批"></transition>
	</start-state>


	<task-node name="DBA审批处理">
		<description>
			auditProcess
		</description>
		<transition to="结束" name="Y"></transition>
		<transition to="打回" name="N"></transition>
	</task-node>

	<task-node name="打回">
		<description>
			back
		</description>
		<transition to="放弃" name="N"></transition>
		<transition to="部门总监审批" name="Y"></transition>
	</task-node>

	<task-node name="部门总监审批">
		<description>
			deptDirectorAudit
		</description>
		<transition to="DBA审批处理" name="Y"></transition>
		<transition to="打回" name="N"></transition>
	</task-node>


	<end-state name="结束">
		<description>
			end
		</description>
	</end-state>

	<end-state name="放弃">
		<description>
			giveUp
		</description>
	</end-state>


</process-definition>