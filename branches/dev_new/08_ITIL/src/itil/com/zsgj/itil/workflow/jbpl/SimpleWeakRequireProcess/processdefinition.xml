<?xml version="1.0" encoding="UTF-8"?>

<process-definition  xmlns=""  name="SimpleWeakRequireProcess">


	<description>
		简单弱电工程申请流程
	</description>


	<start-state name="提出申请">
		<description>
			start
		</description>
		<transition to="部门经理审批"></transition>
	</start-state>


	<task-node name="部门经理审批">
		<description>
			deptManagerAudit
		</description>
		<transition to="行政部审批" name="Y"></transition>
		<transition to="打回" name="N"></transition>
	</task-node>

	<task-node name="行政部审批">
		<description>
			adminDeptAudit
		</description>
		<transition to="运维主管审批" name="Y"></transition>
		<transition to="打回" name="N"></transition>
	</task-node>

	<task-node name="运维主管审批">
		<description>
			operationHeadAudit
		</description>
		<transition to="运维经理审批" name="Y"></transition>
		<transition to="打回" name="N"></transition>
	</task-node>

	<task-node name="运维经理审批">
		<description>
			opeartionManagerAudit
		</description>
		<transition to="结束" name="Y"></transition>
		<transition to="打回" name="N"></transition>
	</task-node>

	<task-node name="打回">
		<description>
			back
		</description>
		<transition to="放弃" name="Y"></transition>
		<transition to="部门经理审批" name="N"></transition>
	</task-node>


	<end-state name="结束">
		<description>
			end
		</description>
	</end-state>

	<end-state name="放弃">
		<description>
			giveUp
		</description>
	</end-state>


</process-definition>