<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="com.zsgj.itil.account.entity.SystemAppAdmin" table="itil_ac_SystemAppAdmin" dynamic-insert="true" dynamic-update="true">
		<id name="id" type="long">
			<column name="id" />
			<generator class="native" />
		</id> 
		<property name="itCode" type="string">
			<column name="itCode"></column>
		</property>
		<property name="realName" type="string">
			<column name="realName" ></column>
		</property>
	</class>
</hibernate-mapping>
