<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="com.zsgj.dcit.entity.EventTypeServiceItem" table="EventTypeServiceItem">
		<id name="id" type="long">
				<column name="id"></column>
				<generator class="native"></generator>
		</id>
		
		<many-to-one name="eventType" class="com.zsgj.dcit.entity.EventType" >
            <column name="eventType" />
        </many-to-one>
        
        <many-to-one name="serviceItem" class="com.zsgj.dcit.entity.ServiceItem" >
            <column name="serviceItem" />
        </many-to-one>
        
	</class>
</hibernate-mapping>
