<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class
		name="com.zsgj.info.bussutil.protal.entity.PortalColumnTemplate"
		table="sys_portal_column_template">
		<id name="id" type="long">
            <column name="id" />
            <generator class="native" />
        </id>
		<property name="name" type="string">
			<column name="name" length="50"></column>
		</property>
		<property name="imageSrc" type="string">
			<column name="imageSrc" length="255"></column>
		</property>
		<property name="columnScale" type="string">
			<column name="columnScale" length="255"></column>
		</property>
		<property name="createTime" type="timestamp" update="false">
			<column name="createTime"></column>
		</property>
		<property name="marker" type="string">
			<column name="marker"></column>
		</property>

	</class>
</hibernate-mapping>
