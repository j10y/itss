<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.xpsoft.oa.model.kpi.HrPaKpiPBC2UserCmp"
        table="hr_pa_kpipbc2usercmp"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="id"
            column="id"
            type="java.lang.Long"
            length="19"
        >
            <generator class="native"/>
        </id>
         <property
            name="pbcName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="pbcName"
            length="150"
            not-null="false"
            unique="false"
        />
        <property
        	name="fromPBC"
        	type="java.lang.String"
        	update="true"
        	insert="true"
        	access="property"
        	column="fromPBC"
        	not-null="false"
        	unique="false"
        />
        <property
        	name="belongUser"
        	type="java.lang.Long"
        	update="true"
        	insert="true"
        	access="property"
        	column="belongUser"
        	not-null="false"
        	unique="false"
        />
         <many-to-one name="frequency" not-null="false" class="com.xpsoft.oa.model.kpi.HrPaDatadictionary" column="frequency"></many-to-one>
         <property
        	name="createPerson"
        	type="java.lang.Long"
        	update="true"
        	insert="true"
        	access="property"
        	column="createPerson"
        	not-null="false"
        	unique="false"
        />
        <property
        	name="createDate"
        	type="java.util.Date"
        	update="true"
        	insert="true"
        	access="property"
        	column="createDate"
        	length="19"
        	not-null="false"
        	unique="false"
        />
        <property
        	name="publishStatus"
        	type="java.lang.Integer"
        	update="true"
        	insert="true"
        	access="property"
        	column="publishStatus"
        	length="19"
        	not-null="false"
        	unique="false"
        />
        <property
        	name="totalScore"
        	type="java.lang.Float"
        	update="true"
        	insert="true"
        	access="property"
        	column="totalScore"
        	length="19"
        	not-null="false"
        	unique="false"
        />
        <property
        	name="modifyPerson"
        	type="java.lang.Long"
        	update="true"
        	insert="true"
        	access="property"
        	column="modifyPerson"
        	not-null="false"
        	unique="false"
        />
        <property
            name="coefficient"
            type="java.lang.Double"
            update="true"
            insert="true"
            access="property"
            column="coefficient"
            length="20"
            not-null="false"
            unique="false"
        />
    </class>
</hibernate-mapping>
