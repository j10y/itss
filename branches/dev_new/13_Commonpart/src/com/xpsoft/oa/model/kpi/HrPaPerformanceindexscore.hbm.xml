<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.xpsoft.oa.model.kpi.HrPaPerformanceindexscore"
        table="hr_pa_performanceindexscore"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="id"
            column="id"
            type="java.lang.Long"
            length="19"
        >
            <generator class="native"/>
        </id>
         <property
            name="piId"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="piId"
            length="19"
            not-null="false"
            unique="false"
        />
        <property
            name="pisScore"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="pisScore"
            length="19"
            not-null="false"
            unique="false"
        />
        <property
            name="pisType"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="pisType"
            length="19"
            not-null="false"
            unique="false"
        />
        <property
            name="pisDesc"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="pisDesc"
            length="500"
            not-null="false"
            unique="false"
        />
        <property
            name="coefficient"
            type="java.lang.Double"
            update="true"
            insert="true"
            access="property"
            column="coefficient"
            length="20"
            not-null="false"
            unique="false"
        />
    </class>
</hibernate-mapping>
