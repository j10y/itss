<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC 
	"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN" 
	"http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="jfchart" extends="struts-default">
		<action name="jfchart_*"  class="jfreeChartAction" method="{1}">
		    <result name="showbar">/WEB-INF/jfreeChart/showbar.jsp</result>
		    <result name="showline">/WEB-INF/jfreeChart/showline.jsp</result>
		    <result name="showpie">/WEB-INF/jfreeChart/showpie.jsp</result>
		    <result name="showtable">/WEB-INF/jfreeChart/showtable.jsp</result>
		</action>
	</package>
</struts>