<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
    
    <struts>
    
    	<package name="ITIL_DCIT" extends="struts-default">  	
    		<action name="noticeAction_*" class="noticeAction"  method="{1}">
    			<result name="contentPage">/content.jsp</result>
    			<result name="listPage">/list.jsp</result>
    			<result name="searchPage">/searchList.jsp</result>
    		</action>
    		<action name="knowledgeAction_*" class="knowledgeAction"  method="{1}">
    			<result name="contentPage">/knowledgeContent.jsp</result>
    			<result name="listPage">/knowledgeList.jsp</result>
    			<result name="searchPage">/knowledgeSearch.jsp</result>
    		</action>
    		<action name="knowFileAction_*" class="knowFileAction"  method="{1}">
    			<result name="contentPage">/knowFileContent.jsp</result>
    			<result name="listPage">/knowFileList.jsp</result>
    			<result name="searchPage">/knowFileSearch.jsp</result>
    		</action>
    		<action name="userAction_*" class="userAction"  method="{1}">
    			<result name="userList">/userList.jsp</result>
    		</action>    		
    	</package>	
    
    </struts>
