<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.xpsoft.oa.model.bandpoor.ProClass"
        table="bp_proclass"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="id"
            column="id"
            type="java.lang.Long"
            length="19"
        >
            <generator class="native"/>
        </id>
         <property
            name="proClassNum"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="proClassNum"
        />
        <property
            name="proClassName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="proClassName"
        />
    </class>
</hibernate-mapping>
