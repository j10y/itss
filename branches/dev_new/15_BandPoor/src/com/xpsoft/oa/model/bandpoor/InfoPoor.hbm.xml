<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.xpsoft.oa.model.bandpoor.InfoPoor"
        table="bp_infopoor"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="id"
            column="id"
            type="java.lang.Long"
            length="19"
        >
            <generator class="native"/>
        </id>
         <property
            name="bandName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="bandName"
        />
         
        <many-to-one
			name="bandId"
			class="com.xpsoft.oa.model.bandpoor.Band"
            not-null="false"
			fetch="select"
		>	
			<column name="bandId" />
		</many-to-one>
		
		<many-to-one
			name="proClassId"
			class="com.xpsoft.oa.model.bandpoor.ProClass"
            not-null="false"
			fetch="select"
		>	
			<column name="proClassId" />
		</many-to-one>
		
		 <property
            name="proClassName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="proClassName"
        />
        
        <many-to-one
			name="saleStoreid"
			class="com.xpsoft.oa.model.bandpoor.SaleStore"
            not-null="false"
			fetch="select"
			lazy="false"
		>	
			<column name="saleStoreid" />
		</many-to-one>
		 <property
            name="saleSroteDesc"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="saleSroteDesc"
        />
        
        <property
            name="saleStoreName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="saleStoreName"
        />
        
        <many-to-one
			name="floorNumId"
			class="com.xpsoft.oa.model.bandpoor.Floor"
            not-null="false"
			fetch="select"
		>	
			<column name="floorNumId" />
		</many-to-one>
		
        <property
            name="floorNumName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="floorNumName"
        />
         <many-to-one
			name="bandStyleId"
			class="com.xpsoft.oa.model.bandpoor.BandStyle"
            not-null="false"
			fetch="select"
		>	
			<column name="bandStyleId" />
		</many-to-one>
		
        <property
            name="bandStyleName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="bandStyleName"
        />
        
         <many-to-one
			name="mainPriceId"
			class="com.xpsoft.oa.model.bandpoor.MainPrice"
            not-null="false"
			fetch="select"
		>	
			<column name="mainPriceId" />
		</many-to-one>
		
		<property
            name="mainPriceName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="mainPriceName"
        />
		
		<property
            name="mainProductName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="mainProductName"
        />
        
		<property
            name="mainProductPicture"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="mainProductPicture"
        />
		
		<property
            name="webSite"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="webSite"
        />
        
        
         <many-to-one
			name="bandBusinessAreaId"
			class="com.xpsoft.oa.model.bandpoor.BusinessArea"
            not-null="false"
			fetch="select"
		>	
			<column name="bandBusinessAreaId" />
		</many-to-one>
		
		<property
            name="bandBusinessAreaName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="bandBusinessAreaName"
        />
        
         <many-to-one
			name="bandChannelID"
			class="com.xpsoft.oa.model.bandpoor.BandChannel"
            not-null="false"
			fetch="select"
		>	
			<column name="bandChannelID" />
		</many-to-one>
		
		<property
            name="bandChannelName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="bandChannelName"
        />
        
         <property
            name="contactUser"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="contactUser"
        />
		
		<property
            name="contactPhone"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="contactPhone"
        />
        
        <property
            name="bandDesc"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="bandDesc"
        />
        
         <property
            name="companyNature"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="companyNature"
        />
        
         <property
            name="companyAddress"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="companyAddress"
        />
        <property
            name="productLine"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="productLine"
        />
        
        <property
            name="infoStatus"
            type="integer"
            update="true"
            insert="true"
            access="property"
            column="infoStatus"
        />
        
		<property
            name="infoType"
            type="integer"
            update="true"
            insert="true"
            access="property"
            column="infoType"
        />
        
        <property
            name="infoSource"
            type="integer"
            update="true"
            insert="true"
            access="property"
            column="infoSource"
        />
        
        <property
            name="creatDate"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="creatDate"
        />
        <many-to-one
			name="creatUser"
			class="com.xpsoft.oa.model.system.AppUser"
            not-null="false"
			fetch="select"
		>	
		   <column name="creatUser" />
		</many-to-one>
		
		<property
            name="mainPriceStart"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="mainPriceStart"
        />
        
        <property
            name="mainPriceEnd"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="mainPriceEnd"
        />
        
    </class>
</hibernate-mapping>
