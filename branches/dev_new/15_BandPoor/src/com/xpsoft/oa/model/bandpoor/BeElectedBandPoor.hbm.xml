<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.xpsoft.oa.model.bandpoor.BeElectedBandPoor"
        table="bp_beelectedbandpoor"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="id"
            column="id"
            type="java.lang.Long"
            length="19"
        >
            <generator class="native"/>
        </id>
         <property
            name="bandName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="bandName"
        />
         
        <many-to-one
			name="bandId"
			class="com.xpsoft.oa.model.bandpoor.Band"
            not-null="false"
			fetch="select"
		>	
			<column name="bandId" />
		</many-to-one>
		
		
        <property
            name="bandScore"
            type="double"
            update="true"
            insert="true"
            access="property"
            column="bandScore"
        />
        
        <property
            name="status"
            type="integer"
            update="true"
            insert="true"
            access="property"
            column="status"
        />
        <property
            name="creatDate"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="creatDate"
        />
        <property
            name="modifyDate"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="modifyDate"
        />
        <many-to-one
			name="createUser"
			class="com.xpsoft.oa.model.system.AppUser"
            not-null="false"
			fetch="select"
		>	
		   <column name="createUser" />
		</many-to-one>
		<many-to-one
			name="modifyUser"
			class="com.xpsoft.oa.model.system.AppUser"
            not-null="false"
			fetch="select"
		>	
		   <column name="modifyUser" />
		</many-to-one>
		
        <property
            name="infoType"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="infoType"
        />
       
       <set name="infoPoors" table="bp_Info_BeEleBandPoor" cascade="save-update" inverse="false" lazy="true">
        	<key column="beElectedBPId"/>
        	<many-to-many fetch="join" class="com.xpsoft.oa.model.bandpoor.InfoPoor" column="infoBPId"/>
       </set> 
         
    </class>
</hibernate-mapping>
