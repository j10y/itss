<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.xpsoft.oa.model.bandpoor.BandLevel"
        table="bp_bandlevel"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="id"
            column="id"
            type="java.lang.Long"
            length="19"
        >
            <generator class="native"/>
        </id>
         <property
            name="levelName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="levelName"
        />
        <property
            name="levelDesc"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="levelDesc"
        />
         <property
            name="startValue"
            type="java.lang.Double"
            update="true"
            insert="true"
            access="property"
            column="startValue"
        />
        <property
            name="endValue"
            type="java.lang.Double"
            update="true"
            insert="true"
            access="property"
            column="endValue"
        />
        <many-to-one
			name="proClassId"
			class="com.xpsoft.oa.model.bandpoor.ProClass"
            not-null="false"
			fetch="select"
		>	
			<column name="proClassId" />
		</many-to-one>
         <property
            name="flag"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="flag"
        />
        <property
            name="proClassName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="proClassName"
        />
    </class>
</hibernate-mapping>
