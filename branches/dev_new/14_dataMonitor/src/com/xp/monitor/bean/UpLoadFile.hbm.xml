<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.xp.monitor.bean" default-access="field">

    <class name="UpLoadFile" table="sys_UpLoadFile" 
    	   dynamic-insert="true" dynamic-update="true">
       
        <id name="id" type="long"  column="id" >
        </id>	
        <property name="realName" type="string" column="realName"/>  
        
        <property name="filePath" type="string" column="filePath"/> 
        
        <property name="createDate" type="timestamp" column="createDate"/>
        
        <property name="createUser" type="string" column="createUser"/> 
        
    </class>
</hibernate-mapping>
