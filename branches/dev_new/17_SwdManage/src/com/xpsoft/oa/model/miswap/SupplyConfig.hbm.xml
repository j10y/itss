<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.xpsoft.oa.model.miswap.SupplyConfig"
        table="supply_config"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="id"
            column="id"
            type="java.lang.Long"
            length="19"
        >
            <generator class="native"/>
        </id>
        <property
            name="supplyInfoSid"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="supply_info_sid"
            not-null="false"
            unique="false"
        />
        <property
            name="receiveTm"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="receive_tm"
            not-null="false"
            unique="false"
        />
        <property
            name="tmId"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="tm_id"
            not-null="false"
            unique="false"
        />
        <property
            name="receiveEmail"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="receive_email"
            not-null="false"
            unique="false"
        />
        <property
            name="emailId"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="email_id"
            not-null="false"
            unique="false"
        />
    </class>
</hibernate-mapping>