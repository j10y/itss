<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.xpsoft.oa.model.system">
	<class name="MrbsArea" table="mrbs_area">
		<id name="id" type="java.lang.Long">
			<generator class="native"/>
		</id>
		<property name="areaName" column="area_name" type="java.lang.String"/>
		<property name="linkman" column="linkman" type="java.lang.String"/>
		<property name="descn" column="descn" type="java.lang.String"/>
		<property name="shortdescn" column="shortdescn" type="java.lang.String"/>
		<property name="flag" column="flag" type="java.lang.Integer"/>
	</class>
</hibernate-mapping>