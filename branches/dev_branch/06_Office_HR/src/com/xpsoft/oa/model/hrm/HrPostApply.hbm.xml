<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.xpsoft.oa.model.hrm.HrPostApply"
        table="hr_post_apply"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="id"
            column="id"
            type="java.lang.Long"
            length="19"
        >
            <generator class="native"/>
        </id>
        
        <many-to-one name="applyUser" not-null="false" class="com.xpsoft.oa.model.system.AppUser" column="applyUser"></many-to-one>
        <property
        	name="gender"
        	type="java.lang.String"
        	update="true"
        	insert="true"
        	access="property"
        	column="gender"
        	not-null="false"
        />
        <property
        	name="age"
        	type="java.lang.Integer"
        	update="true"
        	insert="true"
        	access="property"
        	column="age"
        	not-null="false"
        />
        <property
        	name="deptId"
        	type="java.lang.Long"
        	update="true"
        	insert="true"
        	access="property"
        	column="deptId"
        	not-null="false"
        />
        <property
        	name="deptName"
        	type="java.lang.String"
        	update="true"
        	insert="true"
        	access="property"
        	column="deptName"
        	not-null="false"
        />
        <property
        	name="postId"
        	type="java.lang.Long"
        	update="true"
        	insert="true"
        	access="property"
        	column="postId"
        	not-null="false"
        />
        <property
        	name="postName"
        	type="java.lang.String"
        	update="true"
        	insert="true"
        	access="property"
        	column="postName"
        	not-null="false"
        />
        <property
        	name="accessionTime"
        	type="java.util.Date"
        	update="true"
        	insert="true"
        	access="property"
        	column="accessionTime"
        	not-null="false"
        />
        <property
        	name="proSummary"
        	type="java.lang.String"
        	update="true"
        	insert="true"
        	access="property"
        	column="proSummary"
        	not-null="false"
        />
        <property
        	name="postManagerId"
        	type="java.lang.Long"
        	update="true"
        	insert="true"
        	access="property"
        	column="postManagerId"
        	not-null="false"
        />
        <property
        	name="postManagerName"
        	type="java.lang.String"
        	update="true"
        	insert="true"
        	access="property"
        	column="postManagerName"
        	not-null="false"
        />
        <property
        	name="postManagerAuditDate"
        	type="java.util.Date"
        	update="true"
        	insert="true"
        	access="property"
        	column="postManagerAuditDate"
        	not-null="false"
        />
        <property
        	name="userManagerAuditDate"
        	type="java.util.Date"
        	update="true"
        	insert="true"
        	access="property"
        	column="userManagerAuditDate"
        	not-null="false"
        />
        <property
        	name="publishStatus"
        	type="java.lang.Integer"
        	update="true"
        	insert="true"
        	access="property"
        	column="publishStatus"
        	not-null="false"
        />
        <many-to-one name="createPerson" not-null="false" class="com.xpsoft.oa.model.system.AppUser" column="createPerson"></many-to-one>
        <property
        	name="createDate"
        	type="java.util.Date"
        	update="true"
        	insert="true"
        	access="property"
        	column="createDate"
        	not-null="false"
        />
        <many-to-one name="modifyPerson" not-null="false" class="com.xpsoft.oa.model.system.AppUser" column="modifyPerson"></many-to-one>
        <property
        	name="modifyDate"
        	type="java.util.Date"
        	update="true"
        	insert="true"
        	access="property"
        	column="modifyDate"
        	not-null="false"
        />
    </class>
</hibernate-mapping>
