<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.xpsoft.oa.model.kpi.HrPaAuthorizepbc"
        table="hr_pa_authorizepbc"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="id"
            column="id"
            type="java.lang.Long"
            length="19"
        >
            <generator class="native"/>
        </id>
         <property
            name="pbcId"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="pbcId"
            length="19"
            not-null="false"
            unique="false"
        />
        <property
            name="userId"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="userId"
            length="19"
            not-null="false"
            unique="false"
        />
        <property
            name="authDate"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="authDate"
            length="19"
            not-null="false"
            unique="false"
        />
        <property
            name="authPerson"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="authPerson"
            length="19"
            not-null="false"
            unique="false"
        />
    </class>
</hibernate-mapping>
