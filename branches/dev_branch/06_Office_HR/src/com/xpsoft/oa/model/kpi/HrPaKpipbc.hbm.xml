            column="belongPost"
            length="19"
            not-null="false"
            unique="false"
        />
        <property
            name="frequency"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="frequency"
            length="19"
            not-null="false"
            unique="false"
        />
        <property
            name="createDate"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="createDate"
