            update="true"
            insert="true"
            access="property"
            column="publishStatus"
            length="19"
            not-null="false"
            unique="false"
        />
        <property
            name="totalScore"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="totalScore"
            length="19"
            not-null="false"
            unique="false"
        />
        <property
            name="modifyDate"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="modifyDate"
            length="19"
            not-null="false"
            unique="false"
        />
        <property
            name="modifyPerson"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="modifyPerson"
            length="19"
            not-null="false"
            unique="false"
        />
