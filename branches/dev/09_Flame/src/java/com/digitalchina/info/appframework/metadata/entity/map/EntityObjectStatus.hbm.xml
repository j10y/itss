<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.digitalchina.info.appframework.metadata.entity" default-access="field">

    <class name="EntityObjectStatus" table="sys_EntityObjectStatus" dynamic-insert="true" dynamic-update="true">
       
        <id name="id" type="long"  column="ID" >
            <generator class="native" /> 
        </id>
        			
        <property name="name" type="string" column="name"/>
        <property name="eventFlag" type="integer" column="eventFlag"/>
      
    </class>
</hibernate-mapping>
