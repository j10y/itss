<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping default-access="field">

	<class name="com.digitalchina.itil.actor.entity.CustomerOutUserInfo" table="CustomerOutUserInfo" >
	
        <id name="id" type="long">
            <column name="id" />
            <generator class="native" />
        </id>      
        
         <many-to-one name="customerOut" class="com.digitalchina.itil.actor.entity.Customer">
            <column name="customerOut" />
        </many-to-one>
        
        <many-to-one name="userInfo" class="com.digitalchina.info.framework.security.entity.UserInfo">
	      <column name="userInfo" />
	    </many-to-one>
        
	</class>
</hibernate-mapping>
