<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.digitalchina.itil.config.extlist.entity.MailVolume" table="itil_lst_MailVolume">
      <id name="id" type="java.lang.Long">
			  <generator class="native"/>
      </id>
			<property name="volume" type="java.lang.String">
			  <column name="VOLUME" ></column>
			</property>
			
			<property name="value" type="java.lang.Integer">
			  <column name="VALUE" ></column>
			</property>
	
  </class>
</hibernate-mapping>
