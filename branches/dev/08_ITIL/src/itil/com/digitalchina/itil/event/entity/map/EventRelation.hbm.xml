<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.digitalchina.itil.event.entity.EventRelation" 
  		 table="EventRelation">
      <id name="id" type="long">
			  <generator class="native"/>
      </id>
        <many-to-one name="parentEvent" class="com.digitalchina.itil.event.entity.Event" fetch="join" lazy="false">
            <column name="parentEvent" />
        </many-to-one>
        <many-to-one name="event" class="com.digitalchina.itil.event.entity.Event" fetch="join" lazy="false">
            <column name="event" />
        </many-to-one>	
        <many-to-one name="eventRelationType" class="com.digitalchina.itil.event.entity.EventRelationType" >
            <column name="eventRelationType" />
        </many-to-one>		
  </class>
</hibernate-mapping>
