<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping default-access="field">
	<class name="com.digitalchina.itil.actor.entity.Customer" table="Customer" >
        <id name="id" type="long">
            <column name="id" />
            <generator class="native" />
        </id>
        
        <property name="customerName" type="string">
            <column name="customerName" />
        </property>
        
        <property name="descn" type="string">
            <column name="descn" />
        </property>
        
        <many-to-one name="customerType" class="com.digitalchina.itil.actor.entity.CustomerType">
        	<column name="customerType"></column>
        </many-to-one>
        
         <many-to-one name="department" class="com.digitalchina.info.framework.security.entity.Department" >
            <column name="departCode" />
        </many-to-one>
        
         <many-to-one name="customerPrincipal" class="com.digitalchina.info.framework.security.entity.UserInfo" >
            <column name="customerPrincipal" />
        </many-to-one>
        
	</class>
</hibernate-mapping>
