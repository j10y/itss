<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.digitalchina.itil.config.extlist.entity.SREngineerAndShare" table="itil_lst_SREngineerAndShare">
      <id name="id" type="java.lang.Long">
			  <generator class="native"/>
      </id>
			<many-to-one name="engineer" class="com.digitalchina.info.framework.security.entity.UserInfo">
		      <column name="ENGINEER" />
		    </many-to-one>
			<property name="shareMoney" type="java.lang.Double">
			  <column name="SHAREMONEY" ></column>
			</property>
			<many-to-one name="specialRequire" class="com.digitalchina.itil.require.entity.SpecialRequirement">
		      <column name="SPECIALREQUIRE" />
		    </many-to-one>
	
  </class>
</hibernate-mapping>
