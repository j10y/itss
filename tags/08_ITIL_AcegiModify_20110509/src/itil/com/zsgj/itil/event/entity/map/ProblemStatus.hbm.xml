<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.zsgj.itil.event.entity.ProblemStatus" 
  		 table="ProblemStatus">
      <id name="id" type="long">
			  <generator class="native"/>
      </id>
		<property name="name" type="string">
		  <column name="name" ></column>
		</property>
		<property name="keyword" type="string">
		  <column name="keyword" ></column>
		</property>
		<property name="rank" type="integer">
		  <column name="rank" ></column>
		</property>
		<property name="color" type="string">
		  <column name="color" ></column>
		</property>
		<property name="isclosed" type="integer">
		  <column name="isclosed" ></column>
		</property>
  </class>
</hibernate-mapping>
