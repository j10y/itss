<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="com.zsgj.itil.config.extci.entity.ServiceProvider" table="itil_ci_ServiceProvider" >
        <id name="id" type="long">
            <column name="id" />
            <generator class="native" />
        </id>
        <property name="cisn"/>
        
        <property name="isIn"/>
        
        <many-to-one name="serviceProviderType" class="com.zsgj.itil.config.extci.entity.ServiceProviderType"/>
        
        <property name="name"/>
        
        <property name="postcode"/>
        
        <property name="address"/>
        
        <property name="bank"/>
        
        <property name="bankAccount"/>
        
        <property name="principal"/>
        
        <property name="contactUser"/>
        
        <property name="telNumber"/>
        
        <property name="email"/>
        
        <property name="services"/>
        
        <property name="startServiceDate"/>
        
        <property name="endServiceDate"/>
        
	</class>
</hibernate-mapping>