<WorkBook xmlVersion="20091224"><TableDataMap><TableData name="ds1" class="com.fr.data.impl.DBTableData"><Parameters><Parameter><Attributes name="startDate"/><O><![CDATA[]]></O></Parameter><Parameter><Attributes name="nofilter"/><O><![CDATA[]]></O></Parameter><Parameter><Attributes name="endDate"/><O><![CDATA[]]></O></Parameter><Parameter><Attributes name="type"/><O><![CDATA[]]></O></Parameter><Parameter><Attributes name="cUser"/><O><![CDATA[]]></O></Parameter></Parameters><Attributes maxMemRowCount="-1"/><Connection class="com.fr.data.impl.NameDatabaseConnection"><DatabaseName><![CDATA[itil]]></DatabaseName></Connection><Query><![CDATA[SELECT a.typeName,
a.sum,
a.readTimes,
a.useTimes,
sUserInfos.realName+'/'+sUserInfos.userName AS userInfo
FROM (SELECT TOP 100 aa.typeName AS typeName,
	aa.createUser AS createUser,count(aa.id) AS sum,
	SUM(aa.readTimes) AS readTimes,SUM(aa.useTime) AS useTimes 
 FROM (SELECT * FROM dbo.ITIL_VIEW_KNOWINFO WHERE 1=1
  ${if(len(startDate)==0,nofilter,"and CONVERT(varchar(10),createDate, 120) >= '" + startDate + "'" )}
${if(len(endDate)==0,nofilter,"and 
CONVERT(varchar(10), createDate, 120) <= '" + endDate + "'" )}
    ) aa
GROUP BY aa.typeName,aa.createUser 
ORDER BY SUM(aa.readTimes) ) a 
INNER JOIN sUserInfos ON a.createUser=sUserInfos.id
where 1=1 
${if(len(cUser)==0,nofilter,"and sUserInfos.userName like '%" + cUser + "%'" )}
${if(len(type)==0,nofilter,"and a.typeName = '" + type + "'" )}]]></Query></TableData></TableDataMap><ReportSettings shrinkToFitMode="0" isHorizontalCenterOnPage="true" headerHeight="0.0" footerHeight="0.0"><PaperSize width="9.764055118110237" height="11.693"/><Margin top="0.27" left="0.7499999999999999" bottom="0.27" right="0.7499999999999999"/></ReportSettings><ReportParameterAttr><Attributes showWindow="true" delayPlaying="true" windowPosition="1" align="1"/><PWTitle><![CDATA[参数]]></PWTitle><ParameterUI class="com.fr.report.parameter.CustomParameterUI"><ReportSettings shrinkToFitMode="0" headerHeight="0.0" footerHeight="0.0"><Margin top="0.27" left="0.75" bottom="0.27" right="0.75"/></ReportSettings><ReportParameterAttr><Attributes showWindow="true" delayPlaying="true" windowPosition="0" align="1"/><PWTitle><![CDATA[参数]]></PWTitle><ParameterUI class="com.fr.report.parameter.CustomParameterUI"><ReportSettings headerHeight="0.0" footerHeight="0.0"><Margin top="0.27" left="0.75" bottom="0.27" right="0.75"/></ReportSettings><RowHeight defaultValue="19"><![CDATA[19,19,19,19,19,19,19,19,19,19,19]]></RowHeight><ColumnWidth defaultValue="72"><![CDATA[72,72,72,72,72,72,72,72,72,72,72]]></ColumnWidth><CellElementList><C c="0" r="0"><O><![CDATA[查询]]></O><Widget class="com.fr.report.web.ui.impl.form.FormSubmitButton"><WidgetAttr/></Widget></C></CellElementList></ParameterUI></ReportParameterAttr><ReportWebAttr><EmailManager mailHost="" password="___" user="" fromEmailAddres=""/><ServerPrinter/><Background name="NullBackground"/></ReportWebAttr><Header reportPageType="0"><Background name="NullBackground"/><LeftList/><CenterList/><RightList/></Header><Footer reportPageType="0"><Background name="NullBackground"/><LeftList/><CenterList/><RightList/></Footer><ReportWriteAttr/><RowHeight defaultValue="19"><![CDATA[19,19,19,19,19,19,19,19,19,19,19]]></RowHeight><ColumnWidth defaultValue="72"><![CDATA[76,95,76,106,18,72,72,72,72,72,72]]></ColumnWidth><CellElementList><C c="0" r="0" 