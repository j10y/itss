<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="com.zsgj.itil.train.entity.TrainPlan" table="TrainPlan">
		<id name="id" type="long">
			<column name="id"></column>
			<generator class="native"></generator>
		</id>
		<property name="planName" type="string">
			<column name="planName"></column>
		</property>
		<property name="planDescn" type="string">
			<column name="planDescn"></column>
		</property>
		<property name="planBeginTime" type="date">
			<column name="planBeginTime"></column>
		</property>
		<property name="planEndTime" type="date">
			<column name="planEndTime"></column>
		</property>
		<property name="remark" type="string">
			<column name="remark"></column>
		</property>
		<property name="deleteFlag" type="integer">
			<column name="deleteFlag" default="0"></column>
		</property>
		<property name="status" type="integer">
			<column name="status" default="0"></column>
		</property>
	</class>
</hibernate-mapping>
