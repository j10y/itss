<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping default-access="field">
	<class name="com.digitalchina.itil.event.entity.CCAgentStatusChange" 
			table="CC_LOG1_AGENTSTATUSCHANGE">
		<id name="id" type="long">
				<column name="id"></column>
				<generator class="native"></generator>
		</id>
	
        <property name="agentId" type="string">
			<column name="agentId"></column>
		</property>
		
		<property name="opertype" type="string">
			<column name="opertype"></column>
		</property>
		
		<property name="startTime" type="date">
			<column name="startTime"></column>
		</property>
		
		<property name="endTime" type="date">
			<column name="endTime"></column>
		</property>
		
		<property name="timespan" type="integer">
			<column name="timespan"></column>
		</property>
		
		<property name="millisec" type="string">
			<column name="millisec"></column>
		</property>
		
		<property name="logtag" type="string">
			<column name="logtag"></column>
		</property>
		
        
       
	</class>
</hibernate-mapping>
