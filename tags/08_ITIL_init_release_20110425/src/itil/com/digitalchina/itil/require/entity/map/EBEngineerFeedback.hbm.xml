<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class
		name="com.digitalchina.itil.require.entity.EBEngineerFeedback"
		table="itil_sci_EBEngineerFeedback">
		<id name="id" type="java.lang.Long">
			<generator class="native" />
		</id>
		<property name="feekback" type="java.lang.String">
			<column name="feekback"></column>
		</property>
		<property name="otherInfo" type="java.lang.String">
			<column name="otherInfo"></column>
		</property>
		<property name="attachment" type="java.lang.String">
			<column name="attachment"></column>
		</property>
		<many-to-one name="erpNeed"
			class="com.digitalchina.itil.require.entity.ERP_NormalNeed">
			<column name="erpNeed" />
		</many-to-one>
	</class>
</hibernate-mapping>
