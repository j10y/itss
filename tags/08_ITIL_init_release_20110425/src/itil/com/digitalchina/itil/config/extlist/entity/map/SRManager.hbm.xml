<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.digitalchina.itil.config.extlist.entity.SRManager" table="itil_lst_SRManager">
      <id name="id" type="java.lang.Long">
			  <generator class="native"/>
      </id>
			<many-to-one name="specialRequire" class="com.digitalchina.itil.require.entity.SpecialRequirement">
		      <column name="SPECIALREQUIRE" />
		    </many-to-one>
			<many-to-one name="mainManager" class="com.digitalchina.info.framework.security.entity.UserInfo">
		      <column name="MAINMANAGER" />
		    </many-to-one>
			<many-to-one name="assistanEngineer" class="com.digitalchina.info.framework.security.entity.UserInfo">
		      <column name="ASSISTANENGINEER" />
		    </many-to-one>
		    <property name="isNewFactory" type="java.lang.Integer">
			  <column name="isNewFactory" ></column>
			</property>
	
  </class>
</hibernate-mapping>
