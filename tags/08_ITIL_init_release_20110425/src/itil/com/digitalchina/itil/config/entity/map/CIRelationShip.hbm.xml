<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="com.digitalchina.itil.config.entity.CIRelationShip" table="CIRelationShip">
		<id name="id" type="long">
				<column name="id" />
				<generator class="native" />
		</id>
		<many-to-one name="parentConfigItemType" class="com.digitalchina.itil.config.entity.ConfigItemType">
			<column name="parentConfigItemType"></column>
		</many-to-one>
		<property  name="parentConfigItemCode" type="string">
			<column name="parentConfigItemCode"></column>
		</property>
		<many-to-one name="parentServiceItemType" class="com.digitalchina.itil.service.entity.ServiceItemType">
			<column name="parentServiceItemType"></column>
		</many-to-one>
		<property name="parentServiceItemCode" type="string">
			<column name="parentServiceItemCode"></column>
		</property>
		<many-to-one name="childConfigItemType" class="com.digitalchina.itil.config.entity.ConfigItemType">
			<column name="childConfigItemType"></column>
		</many-to-one>
		<property name="childConfigItemCode" type="string">
			<column name="childConfigItemCode"></column>
		</property>
		<many-to-one name="childServiceItemType" class="com.digitalchina.itil.service.entity.ServiceItemType">
			<column name="childServiceItemType"></column>
		</many-to-one>
		<property name="childServiceItemCode" type="string">
			<column name="childServiceItemCode"></column>
		</property>
		<property name="status">
			<column name="status"></column>
		</property>
		<many-to-one name="relationShipType" class="com.digitalchina.itil.config.entity.CIRelationShipType">
			<column name="relationShipType"></column>
		</many-to-one>
		<many-to-one name="relationShipGrade" class="com.digitalchina.itil.config.entity.CIRelationShipGrade">
			<column name="relationShipGrade"></column>
		</many-to-one>
		<property name="attachQuotiety" type="double">
			<column name="attachQuotiety"></column>
		</property>
		<property name="atechnoInfo" type="string">
			<column name="atechnoInfo" ></column>
		</property>
		<property name="btechnoInfo" type="string">
			<column name="btechnoInfo" ></column>
		</property>
		<property name="otherInfo" type="string">
			<column name="otherInfo" ></column>
		</property>
		
		<many-to-one name="createUser" class="com.digitalchina.info.framework.security.entity.UserInfo">
		      <column name="createUser" />
		</many-to-one>
		<property name="createDate" type="timestamp">
		  <column name="createDate"></column>
		</property>
		<many-to-one name="modifyUser" class="com.digitalchina.info.framework.security.entity.UserInfo">
	      <column name="modifyUser" />
	    </many-to-one>
		<property name="modifyDate" type="timestamp">
		  <column name="modifyDate"></column>
		</property>
	</class>

</hibernate-mapping>
