<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="com.digitalchina.itil.config.entity.CIBatchModifyPlan" table="CIBatchModifyPlan">
		<id name="id" type="long">
				<column name="id" />
				<generator class="native" />
		</id>
		
		<many-to-one name="batchModify" class="com.digitalchina.itil.config.entity.CIBatchModify">
			<column name="batchModify"></column>
		</many-to-one>
		<many-to-one name="maintenanceCIRel" class="com.digitalchina.itil.config.entity.ConfigItem">
			<column name="maintenanceCIRel"></column>
		</many-to-one>
		<many-to-one name="newConfigItem" class="com.digitalchina.itil.config.entity.ConfigItem">
			<column name="newConfigItem"></column>
		</many-to-one>
		<many-to-one name="oldConfigItem" class="com.digitalchina.itil.config.entity.ConfigItem">
			<column name="oldConfigItem"></column>
		</many-to-one>
		<many-to-one name="newCIRelationShip" class="com.digitalchina.itil.config.entity.CIRelationShip">
			<column name="newCIRelationShip"></column>
		</many-to-one>
		<many-to-one name="oldCIRelationShip" class="com.digitalchina.itil.config.entity.CIRelationShip">
			<column name="oldCIRelationShip"></column>
		</many-to-one>
		<property name="descn" type="string">
			<column name="descn"></column>
		</property>
		<property name="startDate" type="timestamp">
			<column name="startDate"></column>
		</property>
		<property name="endDate" type="timestamp">
			<column name="endDate"></column>
		</property>
		<many-to-one name="officer" class="com.digitalchina.info.framework.security.entity.UserInfo">
			<column name="officer"></column>
		</many-to-one>
		<property name="result" type="integer">
			<column name="result"></column>
		</property>
	</class>

</hibernate-mapping>
