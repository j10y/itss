<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.digitalchina.itil.config.extci.entity.Application" table="itil_ci_Application">
      <id name="id" type="java.lang.Long">
			  <generator class="native"/>
      </id>
			<property name="brand" type="java.lang.String">
			  <column name="BRAND" ></column>
			</property>
			<property name="model" type="java.lang.String">
			  <column name="MODEL" ></column>
			</property>
			<property name="appPackage" type="java.lang.String">
			  <column name="APPPACKAGE" ></column>
			</property>
			<many-to-one name="type" class="com.digitalchina.itil.config.extlist.entity.Apptype">
		      <column name="TYPE" />
		    </many-to-one>
			<property name="supportUserMax" type="java.lang.Integer">
			  <column name="SUPPORTUSERMAX" ></column>
			</property>
			<property name="supportUserStd" type="java.lang.Integer">
			  <column name="SUPPORTUSERSTD" ></column>
			</property>
			<property name="cisn" type="java.lang.String">
			  <column name="CISN" ></column>
			</property>
			<many-to-one name="createUser" class="com.digitalchina.info.framework.security.entity.UserInfo">
		      <column name="CREATEUSER" />
		    </many-to-one>
			<property name="createDate" type="date">
			  <column name="CREATEDATE"></column>
			</property>
			<many-to-one name="modifyUser" class="com.digitalchina.info.framework.security.entity.UserInfo">
		      <column name="MODIFYUSER" />
		    </many-to-one>
			<property name="modifyDate" type="date">
			  <column name="MODIFYDATE"></column>
			</property>
	
  </class>
</hibernate-mapping>
