<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
<class name="com.digitalchina.itil.service.entity.SCIRelationShipType" table="SCIRelationShipType" >
        <id name="id" type="long">
            <column name="id" />
            <generator class="native" />
        </id>
        
        <many-to-one name="sciRelationShip" class="com.digitalchina.itil.service.entity.SCIRelationShip">
            <column name="sciRelationShip" />
        </many-to-one>
        
        <many-to-one name="serviceType" class="com.digitalchina.itil.service.entity.ServiceType">
            <column name="serviceType" />
        </many-to-one>
        
	</class>
</hibernate-mapping>
