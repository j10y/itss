<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="com.digitalchina.itil.finance.entity.FinanceCostType" table="ITIL_FinanceCostType" >
        <id name="id" type="long">
            <column name="id" />
            <generator class="native" />
        </id>
        <property name="costTypeCode" type="string">
            <column name="costTypeCode" />
        </property>
        <property name="costTypeName" type="string">
            <column name="costTypeName" />
        </property>
        <property name="isERPType" type="integer">
            <column name="isERPType" />
        </property>
        <property name="isUsedApportion" type="integer">
            <column name="isUsedApportion" />
        </property>
         <property name="isUsualSubjects" type="integer">
            <column name="isUsualSubjects" />
        </property>
         <property name="costDesc" type="string">
            <column name="costDesc" />
        </property>
	</class>
</hibernate-mapping>
