<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping  default-access="field">
    <class name="com.digitalchina.info.appframework.menu.entity.DeptMenuItem" 
    		table="sys_DeptMenuItem"  >
        <id name="id" type="long">
            <column name="id" />
            <generator class="native" />
        </id>
        <many-to-one name="deptMenu" class="com.digitalchina.info.appframework.menu.entity.DeptMenu" 
      			column="deptMenu" />
      	<many-to-one name="templateMenuItem" class="com.digitalchina.info.appframework.menu.entity.TemplateMenuItem" 
      			column="templateMenuItem" />
    </class>
</hibernate-mapping>
