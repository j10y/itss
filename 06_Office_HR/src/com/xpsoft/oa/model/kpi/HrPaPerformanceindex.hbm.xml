<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.xpsoft.oa.model.kpi.HrPaPerformanceindex"
        table="hr_pa_performanceindex"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="id"
            column="id"
            type="java.lang.Long"
            length="19"
        >
            <generator class="native"/>
        </id>
         <property
            name="paName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="paName"
            length="150"
            not-null="false"
            unique="false"
        />
        <many-to-one
			name="type"
			class="com.xpsoft.oa.model.kpi.HrPaDatadictionary"
            not-null="false"
			fetch="select"
		>	
			<column name="paType" />
		</many-to-one>
		<many-to-one
			name="frequency"
			class="com.xpsoft.oa.model.kpi.HrPaDatadictionary"
            not-null="false"
			fetch="select"
		>	
			<column name="paFrequency" />
		</many-to-one>
        <many-to-one
			name="mode"
			class="com.xpsoft.oa.model.kpi.HrPaDatadictionary"
            not-null="false"
			fetch="select"
		>	
			<column name="paMode" />
		</many-to-one>
        <property
            name="paIsOnlyNegative"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="paIsOnlyNegative"
            length="19"
            not-null="false"
            unique="false"
        />
        <property
            name="paDesc"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="paDesc"
            length="500"
            not-null="false"
            unique="false"
        />
        <property
            name="remark"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="remark"
            length="200"
            not-null="false"
            unique="false"
        />
        <property
            name="createDate"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="createDate"
            length="19"
            not-null="false"
            unique="false"
        />
        <many-to-one name="createPerson" not-null="false" class="com.xpsoft.oa.model.system.AppUser" column="createPerson"></many-to-one>
        <property
            name="publishStatus"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="publishStatus"
            length="19"
            not-null="false"
            unique="false"
        />
        <property
            name="modifyDate"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="modifyDate"
            length="19"
            not-null="false"
            unique="false"
        />
        <many-to-one name="modifyPerson" not-null="false" class="com.xpsoft.oa.model.system.AppUser" column="modifyPerson"></many-to-one>
        <property
        	name="baseScore"
        	type="java.lang.Double"
        	update="true"
        	insert="true"
        	access="property"
        	column="baseScore"
        	length="20"
        	not-null="false"
        	unique="false"
        />
        <property
        	name="finalScore"
        	type="java.lang.Double"
        	update="true"
        	insert="true"
        	access="property"
        	column="finalScore"
        	length="20"
        	not-null="false"
        	unique="false"
        />
        <many-to-one name="parentPa" not-null="false" class="com.xpsoft.oa.model.kpi.HrPaPerformanceindex" column="parentId"></many-to-one>
        <property
        	name="fromPi"
        	type="java.lang.Long"
        	update="true"
        	insert="true"
        	access="property"
        	column="fromPi"
        	length="20"
        	not-null="false"
        	unique="false"
        />
    </class>
</hibernate-mapping>
