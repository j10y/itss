<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.zsgj.itil.config.extci.entity.StorageServerEvent" table="itil_ci_StorageServerEvent">
      <id name="id" type="java.lang.Long">
			  <generator class="native"/>
      </id>
			<property name="brand" type="java.lang.String">
			  <column name="BRAND" ></column>
			</property>
			<property name="storageSize" type="java.lang.String">
			  <column name="STORAGESIZE" ></column>
			</property>
			<property name="model" type="java.lang.String">
			  <column name="MODEL" ></column>
			</property>
			<property name="cisn" type="java.lang.String">
			  <column name="CISN" ></column>
			</property>
			<many-to-one name="raidMode1" class="com.zsgj.itil.config.extlist.entity.DiskRaidtype">
		      <column name="RAIDMODE1" />
		    </many-to-one>
			<property name="ipAddress" type="java.lang.String">
			  <column name="IPADDRESS" ></column>
			</property>
			<many-to-one name="raidMode2" class="com.zsgj.itil.config.extlist.entity.DiskRaidtype">
		      <column name="RAIDMODE2" />
		    </many-to-one>
			<property name="sn" type="java.lang.String">
			  <column name="SN" ></column>
			</property>
			<many-to-one name="createUser" class="com.zsgj.info.framework.security.entity.UserInfo">
		      <column name="CREATEUSER" />
		    </many-to-one>
			<property name="createDate" type="date">
			  <column name="CREATEDATE"></column>
			</property>
			<many-to-one name="modifyUser" class="com.zsgj.info.framework.security.entity.UserInfo">
		      <column name="MODIFYUSER" />
		    </many-to-one>
			<property name="modifyDate" type="date">
			  <column name="MODIFYDATE"></column>
			</property>
			<property name="assetsNum" type="java.lang.String">
			  <column name="assetsNum" ></column>
			</property>
	
  </class>
</hibernate-mapping>
