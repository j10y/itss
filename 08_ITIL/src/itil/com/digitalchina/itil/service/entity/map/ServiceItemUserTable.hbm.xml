<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping> 
	<class name="com.digitalchina.itil.service.entity.ServiceItemUserTable" table="ServiceItemUserTable">
        <id name="id" type="long">
            <column name="id" />
            <generator class="native" />
        </id>
        <many-to-one name="serviceItemType" class="com.digitalchina.itil.service.entity.ServiceItemType" fetch="join" lazy="false">
            <column name="serviceItemType" />
        </many-to-one>
         <many-to-one name="serviceItem" class="com.digitalchina.itil.service.entity.ServiceItem">
            <column name="serviceItem" />
        </many-to-one>
         <many-to-one name="systemMainTable" class="com.digitalchina.info.appframework.metadata.entity.SystemMainTable">
            <column name="systemMainTable" />
        </many-to-one>
  		 <property name="className" type="string">
            <column name="className" />
        </property>
        <property name="tableName" type="string">
            <column name="tableName" />
        </property>
        
        <many-to-one name="pagePanel" class="com.digitalchina.info.appframework.pagemodel.entity.PagePanel">
            <column name="pagePanel" />
        </many-to-one>
        
         <many-to-one name="pageListPanel" class="com.digitalchina.info.appframework.pagemodel.entity.PagePanel">
            <column name="pageListPanel" />
        </many-to-one>
        
        <many-to-one name="groupPanel" class="com.digitalchina.info.appframework.pagemodel.entity.PagePanel">
            <column name="groupPanel" />
        </many-to-one>
        <property name="deployFlag" type="integer">
        	<column name="deployFlag"/>
        </property>
	</class>
</hibernate-mapping>
