<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="com.digitalchina.itil.config.entity.ConfigItemExtendInfo" 
			table="ConfigItemExtendInfo" dynamic-insert="true" dynamic-update="true">
			
		<id name="id" type="long">
			<column name="id" />
			<generator class="native" />
		</id>
		
		<many-to-one name="configItem" class="com.digitalchina.itil.config.entity.ConfigItem">
			<column name="configItem"></column>
		</many-to-one>
		
		<many-to-one 
			      name="systemMainTable" 
			      class="com.digitalchina.info.appframework.metadata.entity.SystemMainTable" 
			      column="systemMainTable" fetch="join" lazy="false" />
			      
		<property name="extendDataId" type="long">
			<column name="extendDataId"></column>
		</property>
		
		     
		
	</class>
</hibernate-mapping>
