<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.digitalchina.itil.train.entity" default-access="field">

    <class name="SurveyType" table="TrainSurveyType" dynamic-insert="true" dynamic-update="true">

        <id name="id" type="long"  column="id">
            <generator class="native" /> 
        </id>
       
        <property name="name" type="string" column="name"/>
 		 <many-to-one name="systemMainTable" class="com.digitalchina.info.appframework.metadata.entity.SystemMainTable">
            <column name="systemMainTable" />
        </many-to-one>
    </class>
</hibernate-mapping>
