<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.xpsoft.oa.model.bandpoor.Band"
        table="bp_band"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="id"
            column="id"
            type="java.lang.Long"
            length="19"
        >
            <generator class="native"/>
        </id>
         <property
            name="bandChName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="bandChName"
        />
        <property
            name="bandEnName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="bandEnName"
        />
        <property
            name="bandDesc"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="bandDesc"
        />
        <property
            name="bandStatus"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="bandStatus"
        />
        <property
            name="flag"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="flag"
        />
        
    </class>
</hibernate-mapping>
