<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.xpsoft.oa.model.bandpoor.SaleAssessmentForBP"
        table="bp_saleassessmentforbp"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="id"
            column="id"
            type="java.lang.Long"
            length="19"
        >
            <generator class="native"/>
        </id>
        <many-to-one
			name="bPId"
			class="com.xpsoft.oa.model.bandpoor.BandPoor"
            not-null="false"
			fetch="select"
		>	
			<column name="BPId" />
		</many-to-one>
		
         <property
            name="targetValue"
            type="double"
            update="true"
            insert="true"
            access="property"
            column="targetValue"
        />
        <property
            name="requireValue"
            type="double"
            update="true"
            insert="true"
            access="property"
            column="requireValue"
        />
        
        <property
            name="status"
            type="integer"
            update="true"
            insert="true"
            access="property"
            column="status"
        />
        
        <property
            name="createDate"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="createDate"
        />
        
        <many-to-one
			name="createUser"
			class="com.xpsoft.oa.model.system.AppUser"
            not-null="false"
			fetch="select"
		>	
		   <column name="createUser" />
		</many-to-one>
		
    </class>
</hibernate-mapping>
