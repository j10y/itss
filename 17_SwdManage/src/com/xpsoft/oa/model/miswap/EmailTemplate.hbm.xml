<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.xpsoft.oa.model.miswap.EmailTemplate"
        table="email_template"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="id"
            column="id"
            type="java.lang.Long"
            length="19"
        >
            <generator class="native"/>
        </id>
        <property
            name="name"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="name"
            length="50"
            not-null="false"
            unique="false"
        />
        <property
            name="description"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="description"
            length="200"
            not-null="false"
            unique="false"
        />
        <property
            name="template"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="template"
            length="200"
            not-null="false"
            unique="false"
        />
        <property
            name="preview"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="preview"
            length="200"
            not-null="false"
            unique="false"
        />
        <property
            name="status"
            type="java.lang.Integer"
            update="true"
            insert="true"
            access="property"
            column="status"
            not-null="false"
            unique="false"
        />
    </class>
</hibernate-mapping>