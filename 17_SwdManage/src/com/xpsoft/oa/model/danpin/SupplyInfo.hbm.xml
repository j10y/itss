<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.xpsoft.oa.model.danpin.SupplyInfo"
        table="supply_info"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="sid"
            column="sid"
            type="java.lang.Long"
            length="19"
        >
            <generator class="native"/>
        </id>
         <property
            name="supplyId"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="supply_id"
            length="100"
            not-null="false"
            unique="false"
        />
        <property
            name="companyName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="company_name"
            length="50"
            not-null="false"
            unique="false"
        />
        <property
            name="legalPerson"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="legal_person"
            length="200"
            not-null="false"
            unique="false"
        />
        <property
            name="linker"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="linker"
            length="200"
            not-null="false"
            unique="false"
        />
        <property
            name="linkerPhone"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="linker_phone"
            length="200"
            not-null="false"
            unique="false"
        />
        <property
            name="supplyAddress"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="supply_address"
            length="200"
            not-null="false"
            unique="false"
        />
    </class>
</hibernate-mapping>